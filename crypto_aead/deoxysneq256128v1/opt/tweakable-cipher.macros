s = enc( s , xor( subkeys[16] , t ) );

// Tweakable AES decryption
#define AES_DEC( s , subkeys , realtweak, t)\
t = realtweak;\
s = xor( s , xor( subkeys[ 0] , t ) );t=permute( t , H_PERMUTATION_INV);\
s = mc_inv(s);\
s = dec( s , xor( subkeys[ 1] , mc_inv(t) ) );t=permute( t , H_PERMUTATION_INV);\
