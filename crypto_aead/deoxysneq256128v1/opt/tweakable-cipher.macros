  ONE_KEY_ROUND( subkeys1[ 1], subkeys1[ 2], subkeys2[ 1], subkeys2[ 2] );	ts[ 2] = xor( xor(subkeys1[ 2],subkeys2[ 2]), RCONS[ 2] ); \
