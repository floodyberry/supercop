// One AES round with the same subkey and 8 different tweaks
#define ONE_ROUND( s, subkey, tweak , Round )\
tmp = xor(subkey, tweak );\
s[0] = enc( s[0] , tmp );\
s[1] = enc( s[1] , xor(tmp, tw_c[Round%8][1] ) );\
